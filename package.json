{
  "name": "itone-karaoke",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "NODE_ENV=development tsx server/index.ts",
    "dev:full": "concurrently \"npm run dev:server\" \"wait-on http://localhost:5000 && npm run dev\"",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "build:android": "npm run build && npx cap copy android && npx cap build android",
    "build:ios": "npm run build && npx cap copy ios && npx cap build ios",
    "deploy:android": "npm run build:android && cd android && ./gradlew assembleRelease",
    "deploy:ios": "npm run build:ios && cd ios/App && xcodebuild -workspace App.xcworkspace -scheme App -configuration Release",
    "store:android": "npm run build && npx cap copy android && cd android && ./gradlew bundleRelease",
    "store:ios": "npm run build:ios && cd ios/App && xcodebuild -workspace App.xcworkspace -scheme App -configuration Release -archivePath ./build/iTone.xcarchive archive"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/app": "^7.0.2",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/device": "^7.0.2",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/network": "^7.0.2",
    "@capacitor/share": "^7.0.2",
    "@capacitor/splash-screen": "^7.0.2",
    "@capacitor/status-bar": "^7.0.2",
    "@octokit/rest": "^22.0.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.2.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.3",
    "@stripe/stripe-js": "^7.9.0",
    "@supabase/supabase-js": "^2.56.1",
    "@tanstack/react-query": "^5.61.4",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.0.1",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "esbuild": "^0.27.2",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "lucide-react": "^0.453.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.17.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.55.0",
    "react-icons": "^5.4.0",
    "recordrtc": "^5.6.2",
    "socket.io-client": "^4.8.1",
    "stripe": "^20.2.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "wait-on": "^8.0.1",
    "webrtc-adapter": "^9.0.3",
    "wouter": "^3.3.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/node": "^20.16.11",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/stripe": "^8.0.416",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.0.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^5.4.19"
  }
